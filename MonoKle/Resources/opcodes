;
; INSTRUCTIONS:
;
; stringtoken    string representation of operation
; byteCode       byte representation of operation (OP code)
; nArguments     amount of arguments the instruction needs


struct
<stringToken = return>
<byteCode = 1>
<nArugments = 1>        // Returns a void for (seemingly) no arguments
end




Constants.VOID_RETURN_OP = 23;
Constants.VOID_ReTURN_NAME = "return";
Constants.VOID_RETURN_NARGS = 0;

// MVM				1 -> DoVoidReturn() 2-> DoConstantReturn() 3-> DoNestedReturn()

// Compiler			return -> 1, return 23 -> [2,23], return x + x -> [3,...]

// Script			return, return 23, return x + x



